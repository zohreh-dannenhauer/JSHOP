;; name: Log_ran_problems.lisp
;; #packages: 10 #citys: NUM_CITY
;; #planes: 2 seed:930942051
;;--------------------------------------
(defproblem log-ran-10-1 logistics
   (
 (AIRPLANE plane1)
 (airplane-at plane1 ap2)
 (CITY city1)
 (CITY city2)
 (AIRPORT ap1)
 (AIRPORT ap2)
 (TRUCK t1 city1)
 (truck-at t1 LOC2)
 (TRUCK t2 city2)
 (truck-at t2 LOC3)
 (LOCATION LOC1)
 (IN-CITY LOC1 city1)
 (LOCATION LOC2)
 (IN-CITY LOC2 city1)
 (LOCATION LOC3)
 (IN-CITY LOC3 city1)
 (IN-CITY ap1 city1)
 (IN-CITY ap2 city2)
 (obj-at package1 LOC1)
 (obj-at package2 LOC1)
)

((achieve-goal 
 (obj-at package2 LOC3)
)))


